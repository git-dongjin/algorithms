package p11279maxheap;

public class Main2 {

}

class MaxHeap {
	private int[] tree;
	private int size;
	
	public MaxHeap(int capacity) {
		this.tree = new int[capacity + 1];
	}
	
	public void add(int element) {
		tree[++size] = element;
	}
	
	public int poll() {
		if (size == 0) {
			return 0;
		}
		
		
	}
}