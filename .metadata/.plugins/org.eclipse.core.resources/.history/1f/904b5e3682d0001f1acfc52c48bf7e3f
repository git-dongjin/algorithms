package p11725parent;

import java.io.*;
import java.util.*;
import java.util.stream.IntStream;

public class Main {
	private static int N;
	private static int[] parents;
	private static List[] adList;
	private static boolean[] visited;
	
	public static void main(String[] args) throws IOException {
		System.setIn(new FileInputStream("src/p11725parent/input.txt"));
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		BufferedWriter bw = new BufferedWriter(new OutputStreamWriter(System.out));
		
		N = Integer.parseInt(br.readLine());
		parents = new int[N + 1];
		adList = new List[N + 1];
		for (int i = 1; i < N + 1; i++) {
			adList[i] = new ArrayList<>();
		}
		visited = new boolean[N + 1];
		
		for (int i = 0; i < N - 1; i++) {
			int[] connected = Arrays.stream(br.readLine().split(" ")).mapToInt(Integer::parseInt).toArray();
			adList[connected[0]].add(connected[1]);
			adList[connected[1]].add(connected[0]);
		}
		
		dfs(1);
		
		IntStream.range(2, N + 1).forEach(i -> {
				bw.write(String.valueOf(parents[i]));
				bw.newLine();
			});
		
		bw.flush();
		bw.close();
		br.close();
	}
	
	private static void dfs(int nowNode) {
		
	}
}
